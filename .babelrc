{
  "presets": [
    ["env"],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    [
    "transform-react-jsx", {
      "useBuiltIns": true
    }],
    "transform-react-jsx-source",
    "transform-es2015-parameters",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["transform-regenerator", {
      "async": false
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": [
        "babili"
      ]
    }
  }
}
